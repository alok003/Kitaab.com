        <form class="d-flex" role="search">
          <div class="container">
            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#sinmodal">
              Sign In
            </button>
            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#linmodal">
              Log In
            </button>
            <button type="button" class="btn btn-danger">Google SignIn</button>
          </div>
        </form>
